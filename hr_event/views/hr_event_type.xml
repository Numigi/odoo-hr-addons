<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <record id="view_hr_event_type_tree" model="ir.ui.view">
        <field name="name">HR Event Type List</field>
        <field name="model">hr.event.type</field>
        <field eval="8" name="priority"/>
        <field name="arch" type="xml">
            <tree string="Event Types" editable="top">
                <field name="name"/>
                <field name="active"/>
            </tree>
        </field>
    </record>

    <record id="action_hr_event_type" model="ir.actions.act_window">
        <field name="name">Event Types</field>
        <field name="res_model">hr.event.type</field>
        <field name="type">ir.actions.act_window</field>
        <field name="view_type">form</field>
        <field name="view_mode">list</field>
    </record>

</odoo>
